<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<title>{% block title %} AppSkeleton {% endblock %}</title>

		<meta name="viewport" content="width=device-width, initial-scale=1">

		<link id="bootstrap-style" href="{{ asset('bundles/appskeletoncms/css/bootstrap.min.css') }}" rel="stylesheet">
		<link href="{{ asset('bundles/appskeletoncms/css/bootstrap-responsive.min.css') }}" rel="stylesheet">
		<link id="base-style" href="{{ asset('bundles/appskeletoncms/css/style.css') }}" rel="stylesheet">
		<link id="base-style-responsive" href="{{ asset('bundles/appskeletoncms/css/style-responsive.css') }}" rel="stylesheet">

		<!--[if lt IE 7 ]>
		<link id="ie-style" href="{{ asset('bundles/appskeletoncms/css/style-ie.css') }}" rel="stylesheet">
		<![endif]-->
		<!--[if IE 8 ]>
		<link id="ie-style" href="{{ asset('bundles/appskeletoncms/css/style-ie.css') }}" rel="stylesheet">
		<![endif]-->
		<!--[if IE 9 ]>
		<![endif]-->

		<!--[if lt IE 9]>
		<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
		<![endif]-->

		{% block stylesheets %}{% endblock %}

		<link rel="shortcut icon" href="{{ asset('bundles/appskeletoncms/img/favicon.ico') }}">
	</head>
	<body>
	<!-- start: Header -->
	<div class="navbar">
		<div class="navbar-inner">
			<div class="container-fluid">
				<a class="btn btn-navbar" data-toggle="collapse" data-target=".top-nav.nav-collapse,.sidebar-nav.nav-collapse">
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</a>
				<a class="brand" href="index.html"> <img alt="Perfectum Dashboard" src="{{ asset('bundles/appskeletoncms/img/logo20.png') }}" /> <span class="hidden-phone">Perfectum Dashboard</span></a>

				<!-- start: Header Menu -->
				<div class="nav-no-collapse header-nav">
					<ul class="nav pull-right">
						<li>
							<a class="btn" href="#">
								<i class="icon-wrench icon-white"></i>
							</a>
						</li>
						<!-- start: User Dropdown -->
						<li class="dropdown">
							<a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
								<i class="icon-user icon-white"></i>
								<span class="caret"></span>
							</a>
							<ul class="dropdown-menu">
								<li><a href="#"><i class="icon-user"></i> Profile</a></li>
								<li><a href="login.html"><i class="icon-off"></i> Logout</a></li>
							</ul>
						</li>
						<!-- end: User Dropdown -->
					</ul>
				</div>
				<!-- end: Header Menu -->

			</div>
		</div>
	</div>
	<!-- start: Header -->

	<div class="container-fluid">
		<div class="row-fluid">

			{{ include('AppSkeletonCmsBundle:Elements:mainMenu.html.twig') }}

			<noscript>
				<div class="alert alert-block span10">
					<h4 class="alert-heading">Warning!</h4>
					<p>You need to have <a href="http://en.wikipedia.org/wiki/JavaScript" target="_blank">JavaScript</a> enabled to use this site.</p>
				</div>
			</noscript>

			<div id="content" class="span10">
				<!-- start: Content -->

				<div>
					<hr>
					<ul class="breadcrumb">
						<li>
							<a href="#">Home</a> <span class="divider">/</span>
						</li>
						{% block breadcrumb %}{% endblock %}
					</ul>
					<hr>
				</div>

				<div class="row-fluid">

					{% block body %}{% endblock %}


					<!-- end: Content -->
				</div><!--/#content.span10-->
			</div><!--/fluid-row-->

			<div class="modal hide fade" id="myModal">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal">Ã—</button>
					<h3>Settings</h3>
				</div>
				<div class="modal-body">
					<p>Here settings can be configured...</p>
				</div>
				<div class="modal-footer">
					<a href="#" class="btn" data-dismiss="modal">Close</a>
					<a href="#" class="btn btn-primary">Save changes</a>
				</div>
			</div>

			<div class="clearfix"></div>

			<footer>
				<p>
					<span style="text-align:left;float:left">&copy; <a href="http://clabs.co" target="_blank">creativeLabs</a> 2013</span>
					<span style="text-align:right;float:right">Powered by: <a href="#">Perfectum Dashboard</a></span>
				</p>

			</footer>

		</div><!--/.fluid-container-->

		<!-- start: JavaScript-->

		<script src="{{ asset('bundles/appskeletoncms/js/jquery-1.9.1.min.js') }}"></script>
		<script src="{{ asset('bundles/appskeletoncms/js/jquery-migrate-1.0.0.min.js') }}"></script>

		<script src="{{ asset('bundles/appskeletoncms/js/jquery-ui-1.10.0.custom.min.js') }}"></script>

		<script src="{{ asset('bundles/appskeletoncms/js/jquery.ui.touch-punch.js') }}"></script>

		<script src="{{ asset('bundles/appskeletoncms/js/bootstrap.min.js') }}"></script>

		<script src="{{ asset('bundles/appskeletoncms/js/jquery.cookie.js') }}"></script>

		<script src='{{ asset('bundles/appskeletoncms/js/fullcalendar.min.js') }}'></script>

		<script src='{{ asset('bundles/appskeletoncms/js/jquery.dataTables.min.js') }}'></script>

		<script src="{{ asset('bundles/appskeletoncms/js/excanvas.js') }}"></script>
		<script src="{{ asset('bundles/appskeletoncms/js/jquery.flot.min.js') }}"></script>
		<script src="{{ asset('bundles/appskeletoncms/js/jquery.flot.pie.min.js') }}"></script>
		<script src="{{ asset('bundles/appskeletoncms/js/jquery.flot.stack.js') }}"></script>
		<script src="{{ asset('bundles/appskeletoncms/js/jquery.flot.resize.min.js') }}"></script>

		<script src="{{ asset('bundles/appskeletoncms/js/jquery.chosen.min.js') }}"></script>

		<script src="{{ asset('bundles/appskeletoncms/js/jquery.uniform.min.js') }}"></script>

		<script src="{{ asset('bundles/appskeletoncms/js/jquery.cleditor.min.js') }}"></script>

		<script src="{{ asset('bundles/appskeletoncms/js/jquery.noty.js') }}"></script>

		<script src="{{ asset('bundles/appskeletoncms/js/jquery.elfinder.min.js') }}"></script>

		<script src="{{ asset('bundles/appskeletoncms/js/jquery.raty.min.js') }}"></script>

		<script src="{{ asset('bundles/appskeletoncms/js/jquery.iphone.toggle.js') }}"></script>

		<script src="{{ asset('bundles/appskeletoncms/js/jquery.uploadify-3.1.min.js') }}"></script>

		<script src="{{ asset('bundles/appskeletoncms/js/jquery.gritter.min.js') }}"></script>

		<script src="{{ asset('bundles/appskeletoncms/js/jquery.imagesloaded.js') }}"></script>

		<script src="{{ asset('bundles/appskeletoncms/js/jquery.masonry.min.js') }}"></script>

		<script src="{{ asset('bundles/appskeletoncms/js/jquery.knob.js') }}"></script>

		<script src="{{ asset('bundles/appskeletoncms/js/jquery.sparkline.min.js') }}"></script>

		<script src="{{ asset('bundles/appskeletoncms/js/custom.js') }}"></script>

		{% block javascripts %}{% endblock %}
	</body>
</html>
